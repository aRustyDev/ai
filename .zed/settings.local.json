{
  "context_servers": {
    "ami_eks-ast-grep": {
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/Users/adamsm/code/images/ami-eks:/data",
        "mcp/ast-grep"
      ]
    },
    "ami_eks-docker-memory": {
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/Users/adamsm/code/images/ami-eks/data/mcp/memory:/data",
        "mcp/memory"
      ]
    },
    "ami_eks-docker-mongodb": {
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/Users/adamsm/code/images/ami-eks/data/mcp/mongo:/data",
        "mcp/mongodb"
      ],
      "env": {
        "MDB_MCP_CONNECTION_STRING": "mongodb://localhost:27017/ami-eks"
      }
    },
    "ami_eks-task-orchestrator": {
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/Users/adamsm/code/images/ami-eks:/data",
        "ghcr.io/jpicklyk/task-orchestrator"
      ]
    },
    "ami_eks-just": {
      "source": "custom",
      "command": "just-mcp",
      "args": ["--watch-dir", "/Users/adamsm/code/images/ami-eks","--watch-dir", "/Users/adamsm/code/images/ami-eks/scripts/just"]
    }
  }
}
